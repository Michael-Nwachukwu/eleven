# Privy Configuration
# Get your Privy App ID from https://dashboard.privy.io/
VITE_PRIVY_APP_ID=your-privy-app-id

# ThirdWeb Configuration
# Get these from: https://thirdweb.com/dashboard
VITE_THIRDWEB_CLIENT_ID=your-thirdweb-client-id
THIRDWEB_SECRET_KEY=your-thirdweb-secret-key

# Aeon x402 Payment Configuration
# Set to 'true' to enable Aeon payment processing by default for all QR codes
VITE_ENABLE_AEON_PAYMENTS=true

# Aeon App ID (merchant identifier)
# Use TEST000001 for sandbox testing, or your production App ID
VITE_AEON_APP_ID=TEST000001

# Arbitrum Network Configuration
VITE_DEFAULT_NETWORK=arbitrum
VITE_ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# Database Encryption
# IMPORTANT: Change this to a random string in production!
ENCRYPTION_SECRET=your-super-secret-encryption-key-change-this-in-production

REDIS_URL=value
VERCEL_OIDC_TOKEN=value

